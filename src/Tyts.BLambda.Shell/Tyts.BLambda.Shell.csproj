<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">
	<Import Project="..\..\build\props\Blazor.props" />
	<PropertyGroup>
		<RunAnalyzersDuringLiveAnalysis>False</RunAnalyzersDuringLiveAnalysis>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="wwwroot\appsettings.Development.secrets.json">
		  <CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</EmbeddedResource>
		<EmbeddedResource Include="wwwroot\appsettings.secrets.json">
			<CopyToOutputDirectory>Never</CopyToOutputDirectory>
		</EmbeddedResource>
		<Content Remove="wwwroot\appsettings.Development.secrets.json" />
		<Content Remove="wwwroot\appsettings.secrets.json" />
	</ItemGroup>



	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.*" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="8.0.*" PrivateAssets="all" />
	</ItemGroup>



	<ItemGroup>
	  <ProjectReference Include="..\Tyts.BLambda.Shell.Domain\Tyts.BLambda.Shell.Domain.csproj" />
	</ItemGroup>
</Project>
